{
  "type": "container",
  "id": "u:iframe-container",
  "style": {
    "flex": "1",
    "position": "static",
    "display": "block",
    "overflow": "hidden",
    "height": "calc(100vh - 105px)"
  },
  "body": [
    {
      "type": "html",
      "id": "u:6893bd49d8c2",
      "visibleOn": "!data.comm_ids",
      "html": "<div style='width:100%; text-align:center; color:#999; font-size:16px; padding-top:200px;'>请点击查询进行搜索</div>"
    },
    {
      "type": "flex",
      "id": "u:pagination-container",
      "visibleOn": "false",
      "direction": "row",
      "justify": "flex-start",
      "alignItems": "center",
      "style": {
        "backgroundColor": "#fafafa",
        "gap": "12px",
        "flexWrap": "nowrap",
        "position": "static"
      },
      "items": [
        {
          "type": "button",
          "label": "上一页",
          "level": "default",
          "size": "sm",
          "id": "u:prev-page-btn",
          "disabledOn": "${page <= 1}",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "setValue",
                  "componentId": "u:78013e50fdec",
                  "args": {
                    "value": {
                      "page": "${page - 1}"
                    }
                  }
                },
                {
                  "actionType": "custom",
                  "script": "document.querySelector('[data-id=\\\"loading-mask\\\"]').style.display = 'flex';"
                }
              ]
            }
          }
        },
        {
          "type": "input-number",
          "name": "page",
          "id": "u:page-input",
          "value": 1,
          "min": 1,
          "size": "sm",
          "style": {
            "width": "80px",
            "marginBottom": "0"
          },
          "prefix": "第",
          "suffix": "页",
          "keyboard": true,
          "showSteps": true,
          "step": 1,
          "max": 3
        },
        {
          "type": "button",
          "label": "下一页",
          "level": "default",
          "size": "sm",
          "id": "u:next-page-btn",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "setValue",
                  "componentId": "u:78013e50fdec",
                  "args": {
                    "value": {
                      "page": "${page + 1}"
                    }
                  }
                },
                {
                  "actionType": "custom",
                  "script": "document.querySelector('[data-id=\\\"loading-mask\\\"]').style.display = 'flex';"
                }
              ]
            }
          },
          "disabledOnAction": false
        },
        {
          "type": "divider",
          "direction": "vertical",
          "style": {
            "height": "20px",
            "margin": "0 8px"
          },
          "id": "u:438327f32754"
        },
        {
          "id": "u:693951df2636",
          "type": "tpl",
          "tpl": "每页",
          "style": {},
          "themeCss": {
            "baseControlClassName": {
              "font:default": {
                "fontSize": "14px",
                "color": "#666"
              }
            }
          }
        },
        {
          "type": "select",
          "id": "u:pagesize-select",
          "style": {
            "width": "90px",
            "marginBottom": "0"
          },
          "size": "sm",
          "onEvent": {
            "change": {
              "actions": [
                {
                  "actionType": "setValue",
                  "componentId": "u:78013e50fdec",
                  "args": {
                    "value": {
                      "page": 1
                    }
                  }
                },
                {
                  "actionType": "custom",
                  "script": "document.querySelector('[data-id=\\\"loading-mask\\\"]').style.display = 'flex';"
                }
              ]
            }
          },
          "name": "page_size",
          "value": 20,
          "options": [
            {
              "label": "20条",
              "value": 20
            },
            {
              "label": "50条",
              "value": 50
            },
            {
              "label": "100条",
              "value": 100
            }
          ],
          "multiple": false
        },
        {
          "type": "divider",
          "direction": "vertical",
          "style": {
            "height": "20px",
            "margin": "0 8px"
          },
          "id": "u:efdff8dc0c08"
        },
        {
          "type": "button",
          "label": "选择列",
          "level": "default",
          "size": "sm",
          "icon": "fa fa-columns",
          "id": "u:column-dialog-btn",
          "onEvent": {
            "click": {
              "actions": [
                {
                  "actionType": "dialog",
                  "dialog": {
                    "type": "dialog",
                    "title": "选择要显示的列",
                    "size": "md",
                    "closeOnEsc": true,
                    "body": [
                      {
                        "type": "checkboxes",
                        "name": "tempColumns",
                        "id": "u:temp-columns",
                        "label": "",
                        "options": [
                          {
                            "label": "项目名称",
                            "value": "commname"
                          },
                          {
                            "label": "计费户数",
                            "value": "count1"
                          },
                          {
                            "label": "部门ID",
                            "value": "Organizes"
                          },
                          {
                            "label": "时间戳",
                            "value": "time_stamp"
                          }
                        ],
                        "value": "${selectedColumns}",
                        "columnsCount": 1,
                        "checkAll": true
                      }
                    ],
                    "actions": [
                      {
                        "type": "button",
                        "label": "取消",
                        "level": "default",
                        "actionType": "cancel"
                      },
                      {
                        "type": "button",
                        "label": "确定",
                        "level": "primary",
                        "onEvent": {
                          "click": {
                            "actions": [
                              {
                                "actionType": "setValue",
                                "componentId": "u:78013e50fdec",
                                "args": {
                                  "value": {
                                    "selectedColumns": "${tempColumns}"
                                  }
                                }
                              },
                              {
                                "actionType": "custom",
                                "script": "document.querySelector('[data-id=\\\"loading-mask\\\"]').style.display = 'flex';"
                              },
                              {
                                "actionType": "closeDialog"
                              }
                            ]
                          }
                        },
                        "actionType": "confirm"
                      }
                    ]
                  }
                }
              ]
            }
          },
          "disabledOnAction": false,
          "visibleOn": "false"
        }
      ],
      "themeCss": {
        "baseControlClassName": {
          "border:default": {
            "borderBottom": "1px solid #e8e8e8"
          },
          "padding-and-margin:default": {
            "padding": "8px 15px"
          }
        }
      },
      "isFixedHeight": false,
      "isFixedWidth": false
    },
    {
      "type": "iframe",
      "id": "u:6f1ee8158eed",
      "src": "/ureport/preview?_u=file:kaipiaoqingkuangtongji.xml&comm_ids=${comm_ids}&corp_cost_ids=${corp_cost_ids}&token=${token}&ts=${ts}&start_date=${start_date}&end_date=${end_date}&contract_type=${contract_type}&_i=1&_r=1",
      "width": "100%",
      "animations": {},
      "visibleOn": "!!data.comm_ids",
      "style": {
        "height": "calc(100vh - 105px)"
      }
    },
    {
      "type": "container",
      "id": "loading-mask",
      "body": [
        {
          "type": "html",
          "html": "<div id='loadingdiv' name='loadingdiv'><i class='fa fa-spinner fa-spin'></i> 页面加载中，请稍候...</div>",
          "id": "u:665c71b65e2b"
        }
      ],
      "style": {
        "position": "absolute",
        "top": "0",
        "left": "0",
        "right": "0",
        "bottom": "0",
        "zIndex": 999,
        "display": "none",
        "alignItems": "center",
        "justifyContent": "center"
      },
      "themeCss": {
        "baseControlClassName": {
          "background:default": "rgba(255,255,255,0.8)",
          "font:default": {
            "fontSize": "18px"
          }
        }
      }
    }
  ],
  "themeCss": {
    "baseControlClassName": {
      "border:default": {
        "border": "1px solid #eee"
      }
    }
  },
  "isFixedHeight": false,
  "animations": {}
}